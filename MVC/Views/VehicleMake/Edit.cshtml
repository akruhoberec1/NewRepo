@using Service.Models.DTOs;
@model VehicleMakeDTO;



<h1>Edit Vehicle Make</h1>

<form asp-controller="VehicleMake" asp-action="Update" method="post">
    <div class="form-group">
        <label asp-for="Name" class="control-label"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Abrv" class="control-label"></label>
        <input asp-for="Abrv" class="form-control" />
        <span asp-validation-for="Abrv" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
    <input type="hidden" asp-for="Id" />
</form>
